<div class="row">
	<div class="col-md-12">
		<table>
			<thead>
				<tr>
					<th>Title</th>
					<th>Start</th>
					<th>End</th>
					<th>Allday</th>
					<th colspan="3"></th>
				</tr>
			</thead>
			<tbody>
				<% @events.each do |event| %>
				<tr>
					<td><%= event.title %></td>
					<td><%= event.start %></td>
					<td><%= event.end %></td>
					<td><%= event.allday %></td>
					<td><%= link_to 'Show', event %></td>
					<td><%= link_to 'Edit', edit_event_path(event) %></td>
					<td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>
<div class="row">
	<div class="col-md-6">
		<div id='calendar'data-token="<%= form_authenticity_token %>"></div>
	</div>
	<div class="col-md-6">
		<button type="button" class="btn btn-warning btn-lg btn-block">Warning</button>
	</div>
</div>
<%= render 'modal' %>
